#!/usr/bin/env bds

out := "graph_18.out.txt"
in1 := "graph_18.in1.txt"

# Clean up
[in1, out].rm()

t0 := task(in1 <- []) {
    sys echo TASK_1 | tee '$in1'
}

wait	# Task executed after this wait

# ERROR: Output dependency is a taskId
dep([t0, out] <- in1) {
    sys echo DEP_1 | tee $out
}

goal out
