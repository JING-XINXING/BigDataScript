#!/usr/bin/env bds

out := "graph_19.out.txt"
in1 := "graph_19.in1.txt"

# Clean up
[in1, out].rm()

t0 := task(in1 <- []) {
    sys echo Hello | tee '$in1'
}

wait	# Task executed after this wait

# Input dependency has already been executed, should not execute again
dep(out <- in1) {
    sys date > $out ; echo Bye
}

goal out
